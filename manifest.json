{
  "name": "Script Executor",
  "version": "0.0.1",
  "description": "Executed scripts on a page",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/",
    "menus",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
